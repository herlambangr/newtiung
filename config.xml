<?xml version='1.0' encoding='utf-8'?>
<widget id="com.moc.newtiungmode" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Tiung Mode</name>
    <description>
    </description>
    <author>
        MOC Corp
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="phonegap-version" value="cli-8.0.0" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-compat" spec="^1.2.0" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.4.4">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="24.1.1+" />
    </plugin>
    <plugin name="phonegap-plugin-push" spec="^2.2.3">
        <variable name="ANDROID_SUPPORT_V13_VERSION" value="27.+" />
        <variable name="FCM_VERSION" value="11.6.2" />
    </plugin>
    <plugin name="cordova-plugin-gallery-refresh" spec="^1.0.61" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-clipboard" spec="^1.0.0" />
    <platform name="android">
        <uses-sdk android:maxSdkVersion="27" android:minSdkVersion="19" android:targetSdkVersion="26" />
    </platform>
    <platform name="android">
        <resource-file src="config/google-services.json" target="app/google-services.json" />
    </platform>
    <platform name="android">
        <icon density="ldpi" height="36" platform="android" src="package-assets/36.png" width="36" />
        <icon density="mdpi" height="48" platform="android" src="package-assets/48.png" width="48" />
        <icon density="hdpi" height="72" platform="android" src="package-assets/72.png" width="72" />
        <icon density="xhdpi" height="96" platform="android" src="package-assets/96.png" width="96" />
    </platform>
    <engine name="android" spec="^7.1.4" />
</widget>
