<?xml version='1.0' encoding='utf-8'?>
<widget id="com.moc.newtiungmode" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="PushNotification">
        <param name="android-package" value="com.adobe.phonegap.push.PushPlugin" />
    </feature>
    <feature name="GalleryRefresh">
        <param name="android-package" value="com.ung.galleryrefresh.GalleryRefresh" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <name>Tiung Mode</name>
    <description>
    </description>
    <author>
        MOC Corp
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <uses-sdk android:maxSdkVersion="27" android:minSdkVersion="19" android:targetSdkVersion="26" />
    <resource-file src="google-services.json" target="app/google-services.json" />
    <icon density="ldpi" height="36" platform="android" src="package-assets/36.png" width="36" />
    <icon density="mdpi" height="48" platform="android" src="package-assets/48.png" width="48" />
    <icon density="hdpi" height="72" platform="android" src="package-assets/72.png" width="72" />
    <icon density="xhdpi" height="96" platform="android" src="package-assets/96.png" width="96" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
